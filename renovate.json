{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "extends": ["config:base"],
  
    "fileMatch": [
      "^apps/([^/]+/)+[^/]+\\.ya?ml$"
    ],
  
    "kustomize": {
      "fileMatch": [
        "^apps/([^/]+/)+kustomization\\.ya?ml$"
      ],
      "pinDigests": false
    }
  }
